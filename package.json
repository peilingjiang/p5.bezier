{
  "name": "p5bezier",
  "version": "0.2.6",
  "description": "p5.bezier.js library for everyone.",
  "main": "lib/p5.bezier.min",
  "keywords": [
    "p5.js",
    "Bezier",
    "Computer Graphics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/peilingjiang/p5.bezier.git"
  },
  "author": "Peiling Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peilingjiang/p5.bezier/issues"
  },
  "homepage": "https://github.com/peilingjiang/p5.bezier#readme",
  "devDependencies": {
    "eslint": "^7.11.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "2.1.2",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,md,html,css}\"",
    "build": "webpack --env=production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add -A"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md,html,css}": "prettier --write"
  }
}
