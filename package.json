{
  "name": "p5bezier",
  "version": "0.5.0",
  "description": "Bezier library for canvas-based graphics on the web, built to work with p5.js.",
  "main": "lib/p5.bezier.min.js",
  "keywords": [
    "p5.js",
    "Bezier",
    "Computer Graphics"
  ],
  "repository": {
